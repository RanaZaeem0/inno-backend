{
    "version": 2,
    "builds": [
      { 
        "src": "src/index.ts", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/src/index.ts" 
      }
    ],
    "build": {
    
      "commands": [
        "npx prisma generate"
      ]
    }
  }
  