{
    "version": 2,
    "builds": [
      { 
        "src": "src/index.ts", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/src/index.ts" 
      }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "@database-url"
      },
      "commands": [
        "npx prisma generate"
      ]
    }
  }
  